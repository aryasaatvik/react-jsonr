{
  "name": "@react-jsonr/vite-plugin",
  "version": "0.1.1",
  "description": "Vite plugin to transform JSX/TSX to JSON for react-jsonr",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "vite",
    "plugin",
    "react",
    "json",
    "jsx",
    "tsx",
    "transform"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aryasaatvik/react-jsonr.git",
    "directory": "packages/vite-plugin"
  },
  "author": "Saatvik Arya <aryasaatvik@gmail.com>",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0",
    "vite": "^6.0.0"
  },
  "dependencies": {
    "@babel/generator": "^7.27.0",
    "@babel/parser": "^7.24.0",
    "@babel/traverse": "^7.24.0",
    "@babel/types": "^7.24.0",
    "react-jsonr": "workspace:*"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.5",
    "@types/node": "^20.11.24",
    "@types/react": "^19.0.12",
    "@vitest/coverage-v8": "^3.0.9",
    "eslint": "^8.57.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.3",
    "vitest": "^3.0.9"
  }
}